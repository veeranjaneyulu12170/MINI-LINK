    "framer-motion": "^12.4.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.13.1",
    "mongoose": "^8.11.0",
    "nanoid": "^5.1.2",
    "react": "^18.3.1",
